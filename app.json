{
  "expo": {
    "name": "Filmes Flix",
    "slug": "Filmes-flix",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/splash-dahora.png",
      "resizeMode": "contain",
      "backgroundColor": "#00000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/logotipo.png",
        "backgroundColor": "#00000"
      }
    },
    "plugins": ["expo-font"]
  }
}
